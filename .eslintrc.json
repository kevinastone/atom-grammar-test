{
  "extends": "airbnb",
  "ecmaFeatures": { "destructuring": true },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "no-unused-vars": [2, {"args": "after-used", "argsIgnorePattern": "^_", "varsIgnorePattern": "^_"}],
    "space-before-function-paren": [2, {"anonymous": "never", "named": "never"}],
    "func-names": [0],
    "max-len": 0
  }
}
